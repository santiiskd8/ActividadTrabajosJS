/*Ahora Martha le gustaria que se vea por consola primero "Tenis".*/
let deportesClubMonteGrande = ["futbol", "voley", "basquet", "Tenis"];



// Invertir el orden de los elementos del array
deportesClubMonteGrande.reverse();

// Mostrar el array actualizado en la consola
console.log(deportesClubMonteGrande); // ['tenis', 'basquet', 'voley', 'futbol']
