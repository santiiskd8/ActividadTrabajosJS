/*Rodolfo,para compensar su joda(y no dormir afuera) quiere crear un nuevo array a partir de deportesClubMonteGrande donde cada deporte tenga el prefijo "Deporte: ".*/

let deportesClubMonteGrande = ["futbol", "voley", "basquet", "tenis"];

// Crear un nuevo array donde cada deporte tenga el prefijo "Deporte: "
let deportesConPrefijo = deportesClubMonteGrande.map(deporte => "Deporte: " + deporte);

// Mostrar el nuevo array en la consola
console.log(deportesConPrefijo); // ['Deporte: futbol', 'Deporte: voley', 'Deporte: basquet', 'Deporte: tenis']
