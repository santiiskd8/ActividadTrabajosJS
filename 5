/*A Martha quiere agregar en el array deportesClubMonteGrande el deporte tenis pero quiere
que este antes de voley */
let deportesClubMonteGrande = ["futbol", "voley", "basquet"];

// Insertar "tenis" en la posici√≥n 1 (antes de "voley")
deportesClubMonteGrande.splice(1, 0, "tenis");

// Mostrar el array actualizado en la consola
console.log(deportesClubMonteGrande); // ['futbol', 'tenis', 'voley', 'basquet']
