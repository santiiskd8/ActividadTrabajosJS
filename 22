/*Martha quiere saber la suma de todos los salarios de sus empleados.*/
let sueldos = [
    { nombre: "Juan", salario: 45000 },
    { nombre: "MarÃ­a", salario: 55000 },
    { nombre: "Pedro", salario: 48000 },
    { nombre: "Ana", salario: 60000 }
];

// Calcular la suma total de los sueldos
let sumaSueldos = sueldos.reduce((total, empleado) => total + empleado.salario, 0);

// Mostrar la suma total de los sueldos en la consola
console.log("La suma total de los sueldos es:", sumaSueldos);
